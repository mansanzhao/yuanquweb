{
  "name": "fang",
  "version": "1.0.6",
  "description": "统计图 https://sc.chinaz.com/mobandemo.aspx?downloadid=820213364836 https://sc.chinaz.com/mobandemo.aspx?downloadid=32021115010848",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "pack": "electron-builder",
    "publish": "electron-forge publish",
    "release": "electron-builder"
  },
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "fang_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32"
          ]
        }
      ]
    }
  },
  "author": "fang",
  "license": "ISC",
  "build": {
    "appId": "com.example.app",
    "productName": "FangApp",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "main.js"
    ],
    "extraResources": [
    {
      "from": "node_modules/electron-log",
      "to": "node_modules/electron-log",
      "filter": ["**/*"]
    }
  ],
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "mansanzhao",
        "repo": "yuanquweb"
      },
      {
        "provider": "generic",
        "url": "https://gy.baixincloud.cn/updates"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron-forge/publisher-github": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "electron-log": "^5.2.4",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.3.9"
  }
}
